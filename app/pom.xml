            <artifactId>commons-validator</artifactId>
            <version>1.6</version>
        </dependency>

        <dependency>
            <groupId>org.incode.eurocommercial</groupId>
            <artifactId>relatio-canonical</artifactId>
            <version>${relatio-canonical.version}</version>
